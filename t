sudo iptables -t mangle -D PREROUTING -p udp -m comment --comment "wireguard.nix rule for wg0" -j TRACE; sudo iptables -t mangle -D POSTROUTING -p udp -m mark --mark ${toString fwMark} -m comment --comment "wireguard.nix rule for wg0" -j TRACE; sudo iptables -t raw -D PREROUTING -d ${nodeIPv4}/32 ! -i wg0 -m addrtype ! --src-type LOCAL -m comment --comment "wireguard.nix rule for wg0" -j TRACE
